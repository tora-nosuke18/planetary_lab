  <child link="wheel_left_back_1"/>
  <axis xyz="0.0 1.0 -0.0"/>
</joint>

<joint name="right_back" type="continuous">
  <origin xyz="-0.363731 -0.088 -0.21" rpy="0 0 0"/>
